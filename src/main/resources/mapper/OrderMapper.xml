<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whx.mapper.OrderMapper">

    <update id="agree">
        update `order`
        set station=1
        where id = #{id}
    </update>

    <update id="cancel">
        update `order`
        set station=2
        where id = #{id}
    </update>
</mapper>
